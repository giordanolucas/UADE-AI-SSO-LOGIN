{
  "name": "distribuidas-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "webpack -p --config webpack.config.prod.js",
    "start-dev": "webpack-dev-server --open --config webpack.config.dev.js --port 8081"
  },
  "dependencies": {
    "auth0-js": "^9.10.4",
    "axios": "^0.19.0",
    "bootstrap": "4.3.1",
    "express": "4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.9.1",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "9.0.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "fetch-mock": "7.3.1",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "jest": "24.5.0",
    "json-server": "0.14.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-fetch": "^2.3.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "react-svg-loader": "3.0.1",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
